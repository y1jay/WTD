{
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"extends": [
		"airbnb-base",
		"plugin:node/recommended",
		"plugin:jsonc/recommended-with-json5",
		"prettier",
		"eslint:recommended",
		"plugin:prettier/recommended"
	],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"no-console": "off",
		"prettier/prettier": "off"
	}
}
